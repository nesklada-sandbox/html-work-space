{
  "picture": {
      "prefix": "g-picture",
      "body": [
        "<picture class=\"\">",
        "    <source srcset=\"./avif/$1.avif\" type=\"image/avif\">",
        "    <source srcset=\"./webp/$1.webp\" type=\"image/webp\">",
        "    <img src=\"./img/$1.$2\" alt=\"$3\" loading=\"lazy\">",
        "</picture>"
      ],
      "description": "Snippet for basic web3-img with lazy loadings"
  }
}
